{
  "name": "spryker/newsletter",
  "autoload": {
    "psr-0": {
      "Spryker": "src/",
      "Function": "tests/"
    }
  },
  "require": {
    "spryker/customer": "^2.0.0 || ^3.0.0",
    "spryker/glossary": "^2.0.0",
    "spryker/kernel": "^2.8.0",
    "spryker/mail": "^3.0.0",
    "spryker/util-text": "^1.0.0",
    "spryker/zed-request": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "description": "Newsletter bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/"
    }
  }
}
