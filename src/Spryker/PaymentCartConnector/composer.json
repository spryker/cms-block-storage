{
  "name": "spryker/payment-cart-connector",
  "description": "Payment cart connector module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.0.0"
  },
  "suggest": {
    "spryker/cart": "If you want to use Cart plugins."
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/",
      "Function": "tests/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
