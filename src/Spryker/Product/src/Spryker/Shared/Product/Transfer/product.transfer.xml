<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="ConcreteProduct">
        <property name="idConcreteProduct" type="int" />
        <property name="sku" type="string" />
        <property name="productImageUrl" type="string" />
        <property name="productAbstractSku" type="string" />
        <property name="idProductAbstract" type="int" />
        <property name="taxSet" type="TaxSet" />
        <property name="name" type="string" />
        <property name="attributes" type="array" singular="attribute" />
        <property name="localizedAttributes" type="LocalizedAttributes[]" />
        <property name="isActive" type="bool" />
    </transfer>

    <transfer name="ProductAbstract">
        <property name="idProductAbstract" type="int" />
        <property name="sku" type="string" />
        <property name="attributes" type="array" singular="attribute" />
        <property name="localizedAttributes" type="LocalizedAttributes[]" />
        <property name="isActive" type="bool" />
        <property name="taxSet" type="TaxSet" />
    </transfer>

    <transfer name="LocalizedAttributes">
        <property name="locale" type="Locale" />
        <property name="name" type="string" />
        <property name="attributes" type="array" singular="attribute" />
    </transfer>

    <transfer name="TaxSet">
        <property name="idTaxSet" type="int" />
        <property name="name" type="string" />
        <property name="taxRates" type="TaxRate[]" singular="taxRate" />
    </transfer>

    <transfer name="TaxRate">
        <property name="idTaxRate" type="int" />
        <property name="name" type="string" />
        <property name="rate" type="float" />
    </transfer>

</transfers>

