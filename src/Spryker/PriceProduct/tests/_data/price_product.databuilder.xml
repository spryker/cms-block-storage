<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="PriceProduct">
        <property name="sku" dataBuilderRule="isbn10" />
        <property name="isActive" dataBuilderRule="=true" />
    </transfer>

    <transfer name="MoneyValue">
        <property name="netAmount" dataBuilderRule="100" />
        <property name="grossAmount" dataBuilderRule="=119" />
    </transfer>

</transfers>

