{
  "name": "spryker/state-machine",
  "autoload": {
    "psr-0": {
      "Spryker": "src/",
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "require": {
    "spryker/application": "^2.0.0",
    "spryker/console": "^2.0.0",
    "spryker/graph": "^2.0.0",
    "spryker/kernel": "^2.0.0",
    "spryker/library": "^2.0.0",
    "spryker/propel": "^2.2.0",
    "spryker/symfony": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "^2.0.0"
  },
  "description": "StateMachine bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "config": {
    "sort-packages": true
  }
}
