{% block autosuggest_widget %}
    {{ form_widget(form,  { 'attr': {'class': 'type_asw'} }) }}
    <link href="//raw.githubusercontent.com/devbridge/jQuery-Autocomplete/master/content/styles.css" rel="stylesheet"/>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.11.3/jquery.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery.devbridge-autocomplete/1.2.21/jquery.autocomplete.min.js"></script>
    <script type="text/javascript">
        (function($) {
            jQuery(window).load(function () {
                $('.type_asw').autocomplete({
                    serviceUrl: '{{ url }}',
                    minChars: 1,
                    type: 'GET'
                });
            });
        })(jQuery);
    </script>
{% endblock %}
