{
  "name": "spryker/product-option-discount-connector",
  "description": "ProductOptionDiscountConnector bundle",
  "license": "proprietary",
  "require": {
    "spryker/discount": "^4.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sales": "^5.0.0",
    "spryker/tax": "^4.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/calculation": "If you want to use Calculation plugins you need to install spryker/calculation.",
    "spryker/sales-aggregator": "If you want to use SalesAggregator plugins you need to install spryker/sales-aggregator."
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
