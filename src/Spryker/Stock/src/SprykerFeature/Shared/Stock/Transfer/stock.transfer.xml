<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="StockStockProduct">
        <property name="sku" type="string" default="null" />
        <property name="stockType" type="string" default="null" />
        <property name="quantity" type="string" default="null" />
        <property name="isNeverOutOfStock" type="string" default="null" />
        <property name="idStockProduct" type="int" default="null" />
    </transfer>

    <transfer name="StockStockType">
        <property name="idStock" type="int" default="null" />
        <property name="name" type="string" default="null" />
    </transfer>

</transfers>

