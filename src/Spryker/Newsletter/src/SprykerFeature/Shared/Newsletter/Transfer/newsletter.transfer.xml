<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="NewsletterSubscription">
        <property name="email" type="string" />
        <property name="idCustomer" type="int" />
        <property name="subscriptionTypes" type="NewsletterSubscriptionType[]" singular="subscriptionType" />
    </transfer>

    <transfer name="NewsletterSubscriptionType">
        <property name="idNewsletterSubscriptionType" type="int" />
        <property name="name" type="string" />
    </transfer>

    <transfer name="NewsletterSubscriptionResponse">
        <property name="isSuccess" type="bool" />
        <property name="errors" type="NewsletterSubscriptionError[]" singular="error" />
    </transfer>

    <transfer name="NewsletterSubscriptionError">
        <property name="message" type="string" />
    </transfer>

</transfers>
