<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="InstalledPackage">
        <property name="name" type="string"/>
        <property name="version" type="string"/>
        <property name="license" type="string"/>
        <property name="url" type="string"/>
        <property name="type" type="string"/>
    </transfer>

    <transfer name="InstalledPackages">
        <property name="packages" type="InstalledPackage[]" singular="package" />
    </transfer>

</transfers>
