<?xml version="1.0" encoding="UTF-8"?>
<config>
    <development>
        <pages>
            <api>
                <label>Api</label>
                <title>Api</title>
                <uri>#</uri>
                <pages>
                    <facade-api>
                        <label>Facades</label>
                        <title>Facade Api</title>
                        <bundle>system</bundle>
                        <controller>code</controller>
                        <action>facade-api</action>
                    </facade-api>
                    <zed-api>
                        <label>Zed Controller</label>
                        <title>Zed Controller Api</title>
                        <bundle>system</bundle>
                        <controller>code</controller>
                        <action>zed-api</action>
                    </zed-api>
                    <library-api>
                        <label>Library</label>
                        <title>Library Api</title>
                        <bundle>system</bundle>
                        <controller>code</controller>
                        <action>library-api</action>
                    </library-api>
                </pages>
            </api>
            <git>
                <label>Git</label>
                <title>Git</title>
                <uri>#</uri>
                <pages>
                    <git-log>
                        <label>Log</label>
                        <title>Git log for all repositories</title>
                        <bundle>system</bundle>
                        <controller>code</controller>
                        <action>git-log</action>
                    </git-log>
                </pages>
            </git>
            <dependency>
                <label>Dependency</label>
                <title>Dependency</title>
                <bundle>system</bundle>
                <controller>dependency</controller>
                <action>composer-json-validation</action>
            </dependency>
        </pages>
    </development>
    <home>
        <label>Home</label>
        <title>Home</title>
        <bundle>index</bundle>
        <controller>index</controller>
        <action>index</action>
        <visible>0</visible>
    </home>
    <login-logout>
        <label>Login</label>
        <title>Login</title>
        <bundle>auth</bundle>
        <controller>login</controller>
        <action>index</action>
        <visible>0</visible>
    </login-logout>
</config>
