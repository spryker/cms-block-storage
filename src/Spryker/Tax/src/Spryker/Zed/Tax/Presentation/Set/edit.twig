
{% extends '@Gui/Layout/layout.twig' %}

{% block section_title 'Edit Tax Set' | trans %}

{% block action %}
    {{ backActionButton(url('/tax/set/list'), 'Back to list' | trans) }}

{% endblock %}

{% block content %}

    {% embed '@Gui/Partials/widget.twig' with { widget_title: 'Edit tax set' | trans } %}

        {% block widget_content %}
            {{ form_start(form) }}

            {{ form_row(form.name) }}
            {{ form_row(form.idTaxSet) }}
            {{ form_row(form.taxRates) }}


            <input type="submit" class="btn btn-primary" value="{{ 'Edit' | trans }}" />
            {{ form_end(form) }}
        {% endblock %}

    {% endembed %}
{% endblock %}
