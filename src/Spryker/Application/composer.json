{
  "name": "spryker/application",
  "require": {
    "spryker/config": "^3.0.0",
    "spryker/error-handler": "^2.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/log": "^3.0.0",
    "spryker/monolog": "^2.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/silex": "^2.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/twig": "^3.0.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/util-network": "^1.1.0",
    "spryker/util-text": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "description": "Application bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    },
    "psr-4": {
      "Application\\Module\\": "tests/_support/Module"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
