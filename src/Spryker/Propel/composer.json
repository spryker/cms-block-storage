{
  "name": "spryker/propel",
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    },
    "autoload-dev": {
      "psr-0": {
        "Functional": "tests/",
        "Unit": "tests/"
      },
      "psr-4": {
        "Propel\\Module\\": "tests/_support/Module"
      }
    }
  },
  "require": {
    "propel/propel": "2.0.0-alpha5",
    "spryker/config": "^2.0.0",
    "spryker/console": "^2.0.0",
    "spryker/kernel": "^2.0.0",
    "spryker/library": "dev-beta/bugfix-propel as 2.4.0",
    "spryker/monolog": "^2.0.0",
    "spryker/silex": "^2.0.0",
    "spryker/symfony": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "description": "Propel bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "config": {
    "sort-packages": true
  }
}
