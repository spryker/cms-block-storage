{
  "name": "spryker/customer-group",
  "description": "CustomerGroup bundle",
  "license": "proprietary",
  "require": {
    "spryker/customer": "^4.0.0 || ^5.0.0 || ^6.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-date-time": "^1.0.0",
    "spryker/util-text": "^1.1.0"
  },
  "require-dev": {
    "spryker/assertion": "^3.0.0",
    "spryker/code-sniffer": "*",
    "spryker/country": "*",
    "spryker/locale": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*",
    "spryker/twig": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    },
    "psr-4": {
      "CustomerGroup\\Module\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
