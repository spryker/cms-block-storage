{
  "name": "spryker/product",
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "require": {
    "spryker/kernel": "^2.0.0",
    "spryker/key-builder": "^1.0.0",
    "spryker/locale": "^2.2.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/storage": "^2.0.0",
    "spryker/symfony": "^2.0.0",
    "spryker/touch": "^2.0.0",
    "spryker/url": "^2.2.0",
    "spryker/util-encoding": "^1.0.0",
    "spryker/util-text": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "^2.0.0",
    "spryker/testify": "*"
  },
  "description": "Product bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  }
}
