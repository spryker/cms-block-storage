{% block label_widget %}
    {% set css = 'spryker-form-label' %}
    {% set form_label = false %}
    {% if form.vars.attr.class is defined %}
        {% set css = form.vars.attr.class ~ ' spryker-form-label' %}
    {% endif %}
    <label class="{{ css }}">{{ form.vars.text | raw }}</label>
{% endblock label_widget %}