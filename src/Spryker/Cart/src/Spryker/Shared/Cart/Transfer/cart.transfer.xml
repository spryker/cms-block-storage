<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://static.spryker.com http://static.spryker.com/transfer-01.xsd" >

    <transfer name="Quote">
        <property name="items" type="Item[]" singular="item"/>
        <property name="totals" type="Totals" />
        <property name="expenses" type="Expense[]" singular="expense"/>
    </transfer>

    <transfer name="CartChange">
        <property name="items" type="Item[]" singular="item" />
        <property name="quote" type="Quote" />
    </transfer>

    <transfer name="Item">
        <property name="id" type="int" />
        <property name="sku" type="string" />
        <property name="groupKey" type="string"/>
        <property name="quantity" type="int" />
        <property name="unitGrossPrice" type="int" />
        <property name="sumGrossPrice" type="int" />
    </transfer>

</transfers>

