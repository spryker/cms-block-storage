<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="Alfa">
        <interface>Spryker\Demo\FirstInterface</interface>
        <interface>Spryker\Demo\SecondInterface</interface>
        <property name="properties" type="[]" collection="true" />
        <property name="published" type="boolean" default="true" />
        <property name="user_id" type="integer" />
    </transfer>

    <transfer name="Alfa">
        <interface>Spryker\Demo\ThirdInterface</interface>
        <property name="customers" type="Customer[]" collection="true" />
        <property name="cartItems" type="CartItem[]" collection="true" />
    </transfer>

</transfers>
