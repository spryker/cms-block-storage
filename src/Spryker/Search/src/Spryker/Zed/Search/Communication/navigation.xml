<?xml version="1.0" encoding="UTF-8"?>
<config>
    <search-and-filters>
        <label>Search and Filters</label>
        <title>Search and Filters</title>
        <bundle>search</bundle>
        <controller>filters</controller>
        <action>index</action>
        <icon>fa-search</icon>
        <pages>
            <search-preferences>
                <label>Search Preferences</label>
                <title>Search Preferences</title>
                <bundle>search</bundle>
                <controller>filters</controller>
                <action>index</action>
            </search-preferences>
            <filter-preferences>
                <label>Filter Preferences</label>
                <title>Filter Preferences</title>
                <bundle>search</bundle>
                <controller>filters</controller>
                <action>preferences</action>
            </filter-preferences>
        </pages>
    </search-and-filters>
    <maintenance>
        <pages>
            <search>
                <label>Search</label>
                <title>Search index</title>
                <bundle>search</bundle>
                <controller>maintenance</controller>
                <action>index</action>
                <icon>fa-search</icon>
                <pages>
                    <search-list>
                        <label>List</label>
                        <title>Search list</title>
                        <bundle>search</bundle>
                        <controller>maintenance</controller>
                        <action>list</action>
                        <icon>fa-search</icon>
                        <visible>0</visible>
                    </search-list>
                    <search-key>
                        <label>Key</label>
                        <title>Search key</title>
                        <bundle>search</bundle>
                        <controller>maintenance</controller>
                        <action>key</action>
                        <icon>fa-search</icon>
                        <visible>0</visible>
                    </search-key>
                </pages>
            </search>
        </pages>
    </maintenance>
</config>
