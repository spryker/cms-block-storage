<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="UrlRedirect">
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <property name="idRedirect" type="int" default="null" />
        <property name="toUrl" type="string" default="null" />
        <property name="status" type="int" default="301" />
    </transfer>
    <transfer name="UrlUrl">
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <property name="idUrl" type="int" default="null" />
        <property name="url" type="string" default="null" />
        <property name="fkLocale" type="int" default="null" />
        <property name="itemType" type="string" default="null" />
        <property name="itemId" type="string" default="null" />
        <property name="fkProductId" type="int" default="null" />
        <property name="fkCategoryId" type="int" default="null" />
        <property name="fkPageId" type="int" default="null" />
        <property name="fkRedirectId" type="int" default="null" />
        <property name="resourceType" type="string" default="null" />
        <property name="resourceId" type="string" default="null" />
    </transfer>

</transfers>

