{
  "name": "spryker/maintenance",
  "autoload": {
    "psr-0": {
      "Spryker": "src/",
      "Function": "tests/"
    }
  },
  "require": {
    "spryker/application": "^2.0.0",
    "spryker/config": "^2.2.1",
    "spryker/error-handler": "^1.0.4",
    "spryker/kernel": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "description": "Maintenance bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-0": {
      "Unit": "tests/"
    }
  }
}
