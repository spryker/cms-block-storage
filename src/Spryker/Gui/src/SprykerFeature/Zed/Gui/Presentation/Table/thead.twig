{% if config.headers is defined %}
<thead>
    <tr>
        {% for id, value in config.headers %}
        <th id="{{ id }}">
            {{ value }}
        </th>
        {% endfor %}
        {%  for i in 1..config.emptyHeaders %}
        <th id=""></th>
        {% endfor %}
    </tr>
</thead>
{%  endif %}
