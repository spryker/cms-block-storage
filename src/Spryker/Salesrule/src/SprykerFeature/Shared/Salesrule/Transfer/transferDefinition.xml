<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="SalesruleCode">
        <property name="idSalesruleCode" type="int" default="null" />
        <property name="fkSalesruleCodepool" type="int" default="null" />
        <property name="fkCustomer" type="int" default="null" />
        <property name="code" type="string" default="null" />
        <property name="isActive" type="string" default="null" />
        <property name="createdAt" type="string" default="null" />
        <property name="updatedAt" type="string" default="null" />
    </transfer>
    <transfer name="SalesruleCodePool">
        <property name="idSalesruleCodepool" type="int" default="null" />
        <property name="name" type="string" default="null" />
        <property name="prefix" type="string" default="null" />
        <property name="isReusable" type="string" default="null" />
        <property name="isOncePerCustomer" type="string" default="null" />
        <property name="isRefundable" type="string" default="null" />
        <property name="isBalanced" type="string" default="null" />
        <property name="isVoucher" type="string" default="null" />
        <property name="isActive" type="string" default="null" />
    </transfer>
    <transfer name="SalesruleCondition">
        <property name="idSalesruleCondition" type="int" default="null" />
        <property name="fkSalesrule" type="int" default="null" />
        <property name="conditionn" type="string" default="null" />
        <property name="condition" type="string" default="null" />
        <property name="configuration" type="string" default="null" />
        <property name="createdAt" type="string" default="null" />
        <property name="updatedAt" type="string" default="null" />
    </transfer>
    <transfer name="SalesruleItem">
        <property name="idSalesrule" type="int" default="null" />
        <property name="name" type="string" default="null" />
        <property name="description" type="string" default="null" />
        <property name="displayName" type="string" default="null" />
        <property name="scope" type="string" default="null" />
        <property name="action" type="string" default="null" />
        <property name="amount" type="string" default="null" />
        <property name="isActive" type="string" default="null" />
        <property name="createdAt" type="string" default="null" />
        <property name="updatedAt" type="string" default="null" />
    </transfer>
    <transfer name="SalesruleSalesrule">
        <property name="item" type="string" default="Salesrule\Item" />
    </transfer>

</transfers>

