<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="CalculationDiscount">
        <use name="SprykerFeature\Shared\Discount\Dependency\Transfer\DiscountItemInterface" short="DiscountItemInterface" />
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <interface name="SprykerFeature\Shared\Discount\Dependency\Transfer\DiscountItemInterface" short="DiscountItemInterface" />
        <property name="salesruleId" type="string" default="null" />
        <property name="displayName" type="string" default="null" />
        <property name="amount" type="int" default="0" />
        <property name="type" type="string" default="null" />
        <property name="action" type="string" default="null" />
        <property name="scope" type="string" default="null" />
        <property name="usedCodes" type="array" default="[]" />
    </transfer>
    <transfer name="CalculationDiscountTotals">
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\DiscountTotalItemCollectionInterface" short="DiscountTotalItemCollectionInterface" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\DiscountTotalItemInterface" short="DiscountTotalItemInterface" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\DiscountTotalsInterface" short="DiscountTotalsInterface" />
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <interface name="SprykerFeature\Shared\Calculation\Dependency\Transfer\DiscountTotalsInterface" short="DiscountTotalsInterface" />
        <property name="totalAmount" type="int" default="0" />
        <property name="discountItems" type="string" default="Calculation\DiscountTotalItemCollection" />
    </transfer>
    <transfer name="CalculationExpense">
        <use name="SprykerFeature\Shared\Discount\Dependency\Transfer\DiscountableExpenseInterface" short="DiscountableExpenseInterface" />
        <use name="SprykerFeature\Shared\Discount\Dependency\Transfer\DiscountItemInterface" short="DiscountItemInterface" />
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\ExpenseItemInterface" short="ExpenseItemInterface" />
        <use name="SprykerFeature\Shared\Discount\Dependency\Transfer\DiscountableItemCollectionInterface" short="DiscountableItemCollectionInterface" />
        <interface name="SprykerFeature\Shared\Calculation\Dependency\Transfer\ExpenseItemInterface" short="ExpenseItemInterface" />
        <interface name="SprykerFeature\Shared\Discount\Dependency\Transfer\DiscountableExpenseInterface" short="DiscountableExpenseInterface" />
        <property name="type" type="string" default="null" />
        <property name="grossPrice" type="string" default="null" />
        <property name="priceToPay" type="string" default="null" />
        <property name="taxPercentage" type="string" default="null" />
        <property name="name" type="string" default="null" />
        <property name="discounts" type="string" default="Calculation\DiscountCollection" />
    </transfer>
    <transfer name="CalculationExpenseTotalItem">
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\ExpenseTotalItemInterface" short="ExpenseTotalItemInterface" />
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <interface name="SprykerFeature\Shared\Calculation\Dependency\Transfer\ExpenseTotalItemInterface" short="ExpenseTotalItemInterface" />
        <property name="name" type="string" default="null" />
        <property name="type" type="string" default="null" />
        <property name="grossPrice" type="int" default="0" />
        <property name="priceToPay" type="int" default="0" />
    </transfer>
    <transfer name="CalculationExpenseTotals">
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\ExpenseTotalItemCollectionInterface" short="ExpenseTotalItemCollectionInterface" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\ExpenseTotalItemInterface" short="ExpenseTotalItemInterface" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\ExpenseTotalsInterface" short="ExpenseTotalsInterface" />
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <interface name="SprykerFeature\Shared\Calculation\Dependency\Transfer\ExpenseTotalsInterface" short="ExpenseTotalsInterface" />
        <property name="totalOrderAmount" type="int" default="0" />
        <property name="totalItemAmount" type="int" default="0" />
        <property name="totalAmount" type="int" default="0" />
        <property name="expenseItems" type="string" default="Calculation\ExpenseTotalItemCollection" />
    </transfer>
    <transfer name="CalculationTax">
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\TaxInterface" short="TaxInterface" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\TaxItemCollectionInterface" short="TaxItemCollectionInterface" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\TaxItemInterface" short="TaxItemInterface" />
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <interface name="SprykerFeature\Shared\Calculation\Dependency\Transfer\TaxInterface" short="TaxInterface" />
        <property name="totalAmount" type="int" default="0" />
        <property name="taxRates" type="string" default="Calculation\TaxItemCollection" />
    </transfer>
    <transfer name="CalculationTaxItem">
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\TaxItemInterface" short="TaxItemInterface" />
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <interface name="SprykerFeature\Shared\Calculation\Dependency\Transfer\TaxItemInterface" short="TaxItemInterface" />
        <property name="percentage" type="string" default="null" />
        <property name="amount" type="int" default="0" />
    </transfer>
    <transfer name="CalculationTotals">
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\DiscountTotalsInterface" short="DiscountTotalsInterface" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\ExpenseTotalsInterface" short="ExpenseTotalsInterface" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\TaxInterface" short="TaxInterface" />
        <use name="SprykerFeature\Shared\Calculation\Dependency\Transfer\TotalsInterface" short="TotalsInterface" />
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <interface name="SprykerFeature\Shared\Calculation\Dependency\Transfer\TotalsInterface" short="TotalsInterface" />
        <property name="subtotal" type="int" default="0" />
        <property name="subtotalWithoutItemExpenses" type="int" default="0" />
        <property name="expenses" type="string" default="Calculation\ExpenseTotals" />
        <property name="discount" type="string" default="Calculation\DiscountTotals" />
        <property name="tax" type="string" default="Calculation\Tax" />
        <property name="grandTotalWithDiscounts" type="int" default="0" />
        <property name="grandTotal" type="int" default="0" />
    </transfer>

</transfers>

