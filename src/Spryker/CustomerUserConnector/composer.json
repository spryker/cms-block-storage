{
  "name": "spryker/customer-user-connector",
  "description": "CustomerUserConnector module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.5.0",
    "spryker/user": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "^3.0.0",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/customer": "If you want to use the CustomerTransferUsernameExpanderPlugin plugin"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
