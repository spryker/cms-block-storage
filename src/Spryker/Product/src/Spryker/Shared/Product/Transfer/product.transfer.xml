<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProductConcrete">
        <property name="idProductConcrete" type="int" />
        <property name="sku" type="string" />
        <property name="productImageUrl" type="string" />
        <property name="productAbstractSku" type="string" />
        <property name="idProductAbstract" type="int" />
        <property name="taxRate" type="float" />
        <property name="name" type="string" />
        <property name="attributes" type="array" singular="attribute" />
        <property name="localizedAttributes" type="LocalizedAttributes[]" />
        <property name="isActive" type="bool" />
    </transfer>

    <transfer name="ProductAbstract">
        <property name="idProductAbstract" type="int" />
        <property name="sku" type="string" />
        <property name="attributes" type="array" singular="attribute" />
        <property name="localizedAttributes" type="LocalizedAttributes[]" />
        <property name="isActive" type="bool" />
        <property name="taxRate" type="float" />
        <property name="productImagesSets" type="ProductImageSet[]" singular="productImageSet" />
    </transfer>

    <transfer name="LocalizedAttributes">
        <property name="locale" type="Locale" />
        <property name="name" type="string" />
        <property name="attributes" type="array" singular="attribute" />
    </transfer>

    <transfer name="ProductVariant">
        <property name="localeName" type="string" />
        <property name="name" type="string" />
        <property name="sku" type="string" />
        <property name="attributes" type="array" singular="attribute" />
    </transfer>

    <transfer name="Locale">
        <property name="idLocale" type="int"/>
        <property name="localeName" type="string"/>
        <property name="isActive" type="bool"/>
    </transfer>

</transfers>

