{
  "name": "spryker/file-system",
  "require": {
    "spryker/kernel": "^3.0.0",
    "spryker/locale": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "description": "FileSystem bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Shared\\Customer\\": "tests/SprykerTest/Shared/FileSystem/_support/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    },
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
