<?xml version="1.0"?>
<transfer
    xmlns="http://xsd.spryker.com"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd"
>

    <transfer name="Quote">
        <property name="FactFinderSearchRequest" type="FactFinderSearchRequest"/>
        <property name="FactFinderRecommendationRequest" type="FactFinderRecommendationRequest"/>
        <property name="FactFinderSuggestRequest" type="FactFinderSuggestRequest"/>
        <property name="FactFinderTagCloudRequest" type="FactFinderTagCloudRequest"/>
        <property name="FactFinderTrackingRequest" type="FactFinderTrackingRequest"/>
        <property name="FactFinderSimilarRecordsRequest" type="FactFinderSimilarRecordsRequest"/>
        <property name="FactFinderProductCampaignRequest" type="FactFinderProductCampaignRequest"/>
    </transfer>

    <transfer name="FactFinderSearchRequest">
        <property name="query" type="string"/>
    </transfer>

    <transfer name="FactFinderSearchResponse">
        <property name="FactFinderDataCampaigns" type="FactFinderDataCampaign[]"/>
        <property name="FactFinderDataFilterGroups" type="FactFinderDataFilterGroup[]"/>
        <property name="FactFinderDataBreadCrumbs" type="FactFinderDataBreadCrumb[]"/>
    </transfer>

    <!--FACTFinder\Data\.. Objects-->

    <transfer name="FactFinderDataCampaign">
        <property name="name" type="string"/>
        <property name="category" type="string"/>
        <property name="redirectUrl" type="string"/>
        <property name="pushedProducts" type="FactFinderDataRecord[]"/>
        <property name="feedback" type="array"/>
        <property name="activeQuestions" type="FactFinderDataAdvisorQuestion[]"/>
        <property name="advisorTree" type="FactFinderDataAdvisorQuestion[]"/>
        <property name="hasRedirect" type="bool"/>
    </transfer>

    <transfer name="FactFinderDataFilterGroup">
        <property name="name" type="string"/>
        <property name="detailedLinkCount" type="int"/>
        <property name="unit" type="string"/>
        <property name="isRegularStyle" type="bool"/>
        <property name="isSliderStyle" type="bool"/>
        <property name="isTreeStyle" type="bool"/>
        <property name="isMultiSelectStyle" type="bool"/>
        <property name="hasPreviewImages" type="bool"/>
        <property name="hasSelectedItems" type="bool"/>
        <property name="isSingleHideUnselectedType" type="bool"/>
        <property name="isSingleShowUnselectedType" type="bool"/>
        <property name="isMultiSelectOrType" type="bool"/>
        <property name="isMultiSelectAndType" type="bool"/>
        <property name="isTextType" type="bool"/>
        <property name="isNumberType" type="bool"/>
    </transfer>

    <transfer name="FactFinderDataItem">
        <property name="label" type="string"/>
        <property name="url" type="string"/>
        <property name="selected" type="bool"/>
    </transfer>

    <transfer name="FactFinderDataBreadCrumb">
        <property name="FactFinderDataItem" type="FactFinderDataItem"/>

        <property name="isSearchBreadCrumb" type="bool"/>
        <property name="isFilterBreadCrumb" type="bool"/>
        <property name="fieldName" type="string"/>
    </transfer>

    <transfer name="FactFinderDataRecord">
        <property name="name" type="string"/>
    </transfer>

    <transfer name="FactFinderDataAdvisorQuestion">
        <property name="name" type="string"/>
    </transfer>

    <!--END FACTFinder\Data\.. Objects-->

    <transfer name="FactFinderRecommendationRequest">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderRecommendationResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderSuggestRequest">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderSuggestResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderTagCloudRequest">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderTagCloudResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderTrackingRequest">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderTrackingResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderSimilarRecordsRequest">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderSimilarRecordsResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderProductCampaignRequest">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderProductCampaignResponse">
        <property name="temp" type="string"/>
    </transfer>

    <transfer name="FactFinderCsv">
        <property name="locale" type="string" />
        <property name="type" type="string" />
        <property name="number" type="string" />
        <property name="contents" type="string" />
    </transfer>

</transfer>
