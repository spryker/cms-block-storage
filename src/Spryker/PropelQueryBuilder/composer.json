{
  "name": "spryker/propel-query-builder",
  "description": "PropelQueryBuilder module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.0.0",
    "spryker/propel-orm": "^1.2.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/product": "^5.0.0",
    "spryker/propel": "^3.0.0",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    },
    "psr-4": {
      "Category\\Module\\": "tests/",
      "Testify\\Module\\": "vendor/spryker/Testify/tests/_support/Module/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
