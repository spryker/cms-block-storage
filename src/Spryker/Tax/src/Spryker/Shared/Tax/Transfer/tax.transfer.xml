<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="TaxSetCollection">
        <property name="taxSets" type="TaxSet[]" singular="taxSet" />
    </transfer>

    <transfer name="TaxSet">
        <property name="idTaxSet" type="int" />
        <property name="name" type="string" />
        <property name="taxRates" type="TaxRate[]" singular="taxRate" />
    </transfer>

    <transfer name="TaxRateCollection">
        <property name="taxRates" type="TaxRate[]" singular="taxRate" />
    </transfer>

    <transfer name="TaxRate">
        <property name="idTaxRate" type="int" />
        <property name="name" type="string" />
        <property name="rate" type="float" />
    </transfer>

    <transfer name="Item">
        <property name="unitTaxAmountWithProductOptionAndDiscountAmounts" type="int" />
        <property name="sumTaxAmountWithProductOptionAndDiscountAmounts" type="int" />
    </transfer>

    <transfer name="Expense">
        <property name="unitTaxAmount" type="int" />
        <property name="sumTaxAmount" type="int" />
    </transfer>

</transfers>
