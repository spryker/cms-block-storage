{% import 'interface-macros.php.twig' as macro %}
<?php

namespace {{ namespace }};

{% for use in uses %}
use {{ use }};
{% endfor %}

/**
 * !!! THIS FILE IS AUTO-GENERATED, EVERY CHANGE WILL BE LOST WITH THE NEXT RUN OF TRANSFER GENERATOR
 * !!! DO NOT CHANGE ANYTHING IN THIS FILE
 */
{% if interfaces %}
interface {{ name }} extends {{ interfaces|join(', ') }}
{% else %}
interface {{ name }}
{% endif %}
{
{% for methodName, method  in methods %}
{% if methodName starts with 'set' %}
{{ macro.addSetFor(method) }}
{% endif %}
{% if methodName starts with 'get' %}
{{ macro.addGetFor(method) }}
{% endif %}
{% if methodName starts with 'add' %}
{{ macro.addAdd(method) }}
{% endif %}
{% endfor %}

}
