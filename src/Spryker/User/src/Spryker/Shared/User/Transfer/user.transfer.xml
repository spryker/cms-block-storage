<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="User">
        <property name="idUser" type="int" />
        <property name="username" type="string" />
        <property name="password" type="string" />
        <property name="firstName" type="string" />
        <property name="status" type="string" />
        <property name="lastName" type="string" />
        <property name="lastLogin" type="string" />
    </transfer>

    <transfer name="Collection">
        <property name="user" type="User[]" />
    </transfer>

</transfers>
