<?xml version="1.0" encoding="UTF-8"?>
<config>
    <product>
        <pages>
            <product_relations>
                <label>Product Relations</label>
                <title>Product Relations</title>
                <bundle>product-relation</bundle>
                <controller>list</controller>
                <action>index</action>
                <visible>2</visible>

                <pages>
                    <product-relations-create>
                        <label>Create new Product Relation</label>
                        <title>Create new Product Relation</title>
                        <bundle>product-relation</bundle>
                        <controller>create</controller>
                        <action>index</action>
                        <visible>0</visible>
                    </product-relations-create>

                    <product-relations-edit>
                        <label>Edit Product Relation</label>
                        <title>Edit Product Relation</title>
                        <bundle>product-relation</bundle>
                        <controller>edit</controller>
                        <action>index</action>
                        <visible>0</visible>
                    </product-relations-edit>

                    <product-relations-view>
                        <label>View Product Relation</label>
                        <title>View Product Relation</title>
                        <bundle>product-relation</bundle>
                        <controller>view</controller>
                        <action>index</action>
                        <visible>0</visible>
                    </product-relations-view>
                </pages>
            </product_relations>
        </pages>
    </product>
</config>
