{
  "name": "spryker/application",
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    },
    "psr-4": {
      "Application\\Module\\": "tests/_support/Module"
    }
  },
  "require": {
    "spryker/assertion": "^2.0.0",
    "spryker/auth": "^2.0.2",
    "spryker/config": "^2.2.0",
    "spryker/console": "^2.0.0",
    "spryker/gui": "^2.0.0",
    "spryker/kernel": "^2.5.0",
    "spryker/library": "^2.0.0",
    "spryker/log": "^2.0.0",
    "spryker/monolog": "^2.0.0",
    "spryker/new-relic": "^2.0.0",
    "spryker/propel": "^2.0.0",
    "spryker/session": "^2.1.2",
    "spryker/silex": "^2.0.0",
    "spryker/symfony": "^2.0.0",
    "spryker/twig": "^2.1.0",
    "spryker/url": "^2.0.0",
    "spryker/zed-request": "^2.0.0",
    "spryker/zend": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "description": "Application bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    }
  }
}
