{
  "name": "spryker/testify",
  "autoload": {
    "psr-0": {
      "Spryker": "src/",
      "Generated": "src/",
      "Orm": "src/"
    },
    "psr-4": {
      "Testify\\Module\\": "tests/_support/Module"
    }
  },
  "require": {
    "codeception/codeception": "~2.1.5",
    "php-mock/php-mock": "~1.0.1"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "description": "Testify bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
