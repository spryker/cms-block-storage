<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="AclGroup">
        <property name="idAclGroup" type="int" />
        <property name="name" type="string" />
    </transfer>

    <transfer name="Groups">
        <property name="groups" type="AclGroup[]" singular="group" />
    </transfer>

    <transfer name="AclRole">
        <property name="idAclRole" type="int" />
        <property name="name" type="string" />
        <property name="idGroup" type="int" />
        <property name="rules" type="string" />
    </transfer>

    <transfer name="Roles">
        <property name="roles" type="AclRole[]" singular="role" />
    </transfer>

    <transfer name="AclRule">
        <property name="idAclRule" type="int" />
        <property name="bundle" type="string" />
        <property name="controller" type="string" />
        <property name="action" type="string" />
        <property name="type" type="string" />
        <property name="fkAclRole" type="int" />
    </transfer>

    <transfer name="Rules">
        <property name="rules" type="AclRule[]" singular="rule" />
    </transfer>

</transfers>

