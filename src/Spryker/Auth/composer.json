{
  "name": "spryker/auth",
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "require": {
    "spryker/kernel": "^2.0.0",
    "spryker/session": "^2.0.0",
    "spryker/silex": "^2.0.0",
    "spryker/symfony": "^2.0.0",
    "spryker/user": "^2.0.0",
    "spryker/util-text": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/config": "*",
    "spryker/propel-orm": "*",
    "spryker/testify": "*"
  },
  "description": "Auth bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/",
      "ClientUnit": "tests/"
    }
  }
}
