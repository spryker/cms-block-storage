<ul>
    {% for node in nodes %}
        <li id="navigation-node-{{ node.navigationNode.idNavigationNode }}" data-id-navigation-node="{{ node.navigationNode.idNavigationNode }}" class="jstree-open" data-jstree='{"type":"category"}'>
            {{ node.navigationNode.navigationNodeLocalizedAttributes[0].title }}
            {% include '@NavigationGui/Tree/_partials/nodes.twig' with {nodes: node.children} %}
        </li>
    {% endfor %}
</ul>
