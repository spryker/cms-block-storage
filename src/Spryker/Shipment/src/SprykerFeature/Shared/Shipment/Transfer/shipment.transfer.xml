<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="ShipmentCarrier">
        <property name="idShipmentCarrier" type="int" />
        <property name="fkGlossaryKeyCarrierName" type="int" />
        <property name="name" type="string" />
        <property name="isActive" type="bool" />
    </transfer>

    <transfer name="ShipmentMethod">
        <property name="idShipmentMethod" type="int" />
        <property name="fkShipmentCarrier" type="int" />
        <property name="fkGlossaryKeyMethodName" type="int" />
        <property name="fkGlossaryKeyMethodDescription" type="int" />
        <property name="name" type="string" />
        <property name="price" type="int" />
        <property name="isActive" type="bool" />
    </transfer>

    <transfer name="Order">
        <property name="shipmentMethod" type="ShipmentMethod" singular="shipmentMethos" />
    </transfer>

</transfers>
