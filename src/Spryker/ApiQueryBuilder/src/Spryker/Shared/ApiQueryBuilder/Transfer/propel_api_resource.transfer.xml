<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ApiQueryBuilderCollectionQuery">
        <property name="resourceType" type="string" />
        <property name="resourceCriteria" type="PropelQueryBuilderCriteria" />
    </transfer>

    <transfer name="ApiQueryBuilderItemQuery">
        <property name="resourceType" type="string" />
        <property name="resourceId" type="string" />
        <property name="resourceData" type="ApiData" />
        <property name="table" type="PropelQueryBuilderTable"/>
        <property name="selectedColumns" type="PropelQueryBuilderColumn[]" singular="selectedColumn"/>
    </transfer>


</transfers>