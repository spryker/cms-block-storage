{
  "name": "spryker/payone",
  "description": "Payone module",
  "license": "proprietary",
  "require": {
    "spryker/config": "^3.0.0",
    "spryker/guzzle": "^2.1.0",
    "spryker/kernel": "^3.0.0",
    "spryker/oms": "^6.0.0 || ^7.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/refund": "^4.0.0 || ^5.0.0",
    "spryker/sales": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/checkout": "If you want to use Checkout plugins."
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/",
      "Function": "tests/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
