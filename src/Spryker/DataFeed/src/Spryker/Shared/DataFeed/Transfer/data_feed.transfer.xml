<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="DataFeedCondition">
        <property name="productFeedJoin" type="ProductFeedJoin" />
        <property name="categoryFeedJoin" type="CategoryFeedJoin" />
        <property name="priceFeedJoin" type="PriceFeedJoin" />
        <property name="stockFeedJoin" type="StockFeedJoin" />
        <property name="pagination" type="DataFeedPagination" />
        <property name="dateFilter" type="DataFeedDateFilter" />
        <property name="locale" type="Locale" />
    </transfer>

    <transfer name="ProductFeedJoin">
        <property name="isJoinPrice" type="bool" />
        <property name="isJoinCategory" type="bool" />
        <property name="isJoinImage" type="bool" />
        <property name="isJoinVariant" type="bool" />
        <property name="isJoinOption" type="bool" />
    </transfer>

    <transfer name="CategoryFeedJoin">
        <property name="isJoinProduct" type="bool" />
    </transfer>

    <transfer name="PriceFeedJoin">
        <property name="isJoinType" type="bool" />
    </transfer>

    <transfer name="StockFeedJoin">
        <property name="isJoinProduct" type="bool" />
    </transfer>

    <transfer name="DataFeedPagination">
        <property name="limit" type="int" />
        <property name="offset" type="int" />
    </transfer>

    <transfer name="DataFeedDateFilter">
        <property name="updatedFrom" type="string" />
        <property name="updatedTo" type="string" />
    </transfer>

</transfers>
