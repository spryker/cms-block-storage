<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="QueueMessage">
        <property name="id" type="string" />
        <property name="type" type="string" />
        <property name="payload" type="array" />
        <property name="rawMessage" type="array" />
    </transfer>

    <transfer name="AmqpParameter">
        <property name="host" type="string"/>
        <property name="port" type="int"/>
        <property name="user" type="string"/>
        <property name="password" type="string"/>
        <property name="vhost" type="string"/>
    </transfer>

</transfers>
