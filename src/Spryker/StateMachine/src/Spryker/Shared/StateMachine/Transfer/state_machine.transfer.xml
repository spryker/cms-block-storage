<?xml version="1.0"?>
<transfers xmlns="spryker:transfer"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer http://static.spryker.com/transfer-01.xsd">

    <transfer name="StateMachineItem">
        <property name="identifier" type="int"/>
        <property name="idStateMachineProcess" type="string" />
        <property name="idItemState" type="string" />
        <property name="processName" type="string" />
        <property name="stateMachineName" type="string" />
        <property name="stateName" type="string" />
        <property name="eventName" type="string" />
        <property name="createdAt" type="string" />
