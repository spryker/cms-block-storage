    }
  },
  "require": {
    "spryker/gui": "^2.0.0",
    "spryker/installer": "^2.0.0 || ^3.0.0",
    "spryker/kernel": "^2.0.0",
    "spryker/log": "^2.0.0",
    "spryker/messenger": "^2.0.0",
    "spryker/user": "^2.0.0"
