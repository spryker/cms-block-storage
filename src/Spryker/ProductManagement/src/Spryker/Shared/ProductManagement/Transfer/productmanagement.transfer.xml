<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="https://static.spryker.com https://static.spryker.com/transfer-01.xsd" >

    <transfer name="ZedProductConcrete">
        <property name="idProductConcrete" type="int" />
        <property name="sku" type="string" />
        <property name="isActive" type="bool" />
        <property name="attributes" type="array" singular="attribute" />
        <property name="fkProductAbstract" type="int" />
        <property name="abstractSku" type="string" />
        <property name="localizedAttributes" type="LocalizedAttributes[]" />
        <property name="taxRate" type="float" />
        <property name="price" type="PriceProductConcrete" />
        <property name="stock" type="int" />
    </transfer>

    <transfer name="ProductManagementAttribute">
        <property name="idProductManagementAttribute" type="int" />
    </transfer>

    <transfer name="ProductManagementAttributeLocalized">
        <property name="idProductManagementAttributeLocalized" type="int" />
    </transfer>

    <transfer name="ProductManagementAttributeMetadata">
        <property name="idProductManagementAttributeMetadata" type="int" />
    </transfer>

    <transfer name="ProductManagementAttributeInput">
        <property name="idProductManagementAttributeInput" type="int" />
    </transfer>

    <transfer name="ProductManagementAttributeType">
        <property name="idProductManagementAttributeType" type="int" />
    </transfer>

    <transfer name="ProductManagementAttributeValue">
        <property name="idProductManagementAttributeValue" type="int" />
    </transfer>
</transfers>

