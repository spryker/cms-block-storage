{
  "name": "spryker/maintenance",
  "description": "Maintenance bundle",
  "license": "proprietary",
  "require": {
    "spryker/config": "^3.0.0",
    "spryker/error-handler": "^2.0.0",
    "spryker/kernel": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/",
      "Function": "tests/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
